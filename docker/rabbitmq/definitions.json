{
  "rabbit_version": "3.12.0",
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "exchanges": [
    {
      "name": "services.requests",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "services.responses",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "queues": [
    {
      "name": "transport.requests",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    },
    {
      "name": "catering.requests",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    },
    {
      "name": "merchandising.requests",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    },
    {
      "name": "services.responses.platform",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "services.requests",
      "vhost": "/",
      "destination": "transport.requests",
      "destination_type": "queue",
      "routing_key": "service.request.transport",
      "arguments": {}
    },
    {
      "source": "services.requests",
      "vhost": "/",
      "destination": "catering.requests",
      "destination_type": "queue",
      "routing_key": "service.request.catering",
      "arguments": {}
    },
    {
      "source": "services.requests",
      "vhost": "/",
      "destination": "merchandising.requests",
      "destination_type": "queue",
      "routing_key": "service.request.merchandising",
      "arguments": {}
    },
    {
      "source": "services.responses",
      "vhost": "/",
      "destination": "services.responses.platform",
      "destination_type": "queue",
      "routing_key": "service.response",
      "arguments": {}
    }
  ]
}
